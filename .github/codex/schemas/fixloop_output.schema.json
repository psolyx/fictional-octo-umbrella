{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "status": {
      "type": "string",
      "enum": ["changed", "no_change"]
    },
    "summary": {
      "type": "string"
    },
    "touched_files": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["status", "summary", "touched_files"]
}
